        <result column="remote_addr" property="remoteAddr" />
        <result column="exec_time" property="execTime" />
        <result column="is_exception" property="isException" />
        <result column="exception_info" property="exceptionInfo" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        status,
        create_by,
        create_date,
        update_by,
        update_date,
        remarks,
        module_name, business_type, exec_method, remote_url, request_method, remote_addr, exec_time, is_exception, exception_info
    </sql>

</mapper>
