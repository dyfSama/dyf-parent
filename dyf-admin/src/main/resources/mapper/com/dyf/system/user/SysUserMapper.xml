        <result column="update_date" property="updateDate" jdbcType="TIMESTAMP"/>
        <result column="remarks" property="remarks" jdbcType="VARCHAR"/>
    </resultMap>


    <sql id="Base_Column_List">
        id
        ,
        dept_id,
        user_name,
        real_name,
        password,
        mobile,
        email,
        gender,
        /*        avatar,
        */ birthday,
        status,
        create_by,
        create_date,
        update_by,
        update_date,
        remarks
    </sql>


</mapper>
